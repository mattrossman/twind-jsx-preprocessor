{
	"name": "twind-jsx-preprocessor",
	"author": "MapleLeaf",
	"module": "dist/twind-jsx-preprocessor.esm.js",
	"version": "0.1.0",
	"license": "MIT",
	"main": "dist/index.js",
	"typings": "dist/index.d.ts",
	"scripts": {
		"start": "tsdx watch",
		"build": "tsdx build",
		"test": "tsdx test",
		"lint": "tsdx lint",
		"prepare": "tsdx build",
		"size": "size-limit",
		"analyze": "size-limit --why"
	},
	"devDependencies": {
		"husky": "^5.1.3",
		"tsdx": "^0.14.1",
		"tslib": "^2.1.0",
		"typescript": "^4.2.2"
	},
	"peerDependencies": {},
	"files": [
		"dist",
		"src"
	],
	"engines": {
		"node": ">=14"
	},
	"husky": {
		"hooks": {
			"pre-commit": "tsdx lint"
		}
	},
	"prettier": {
		"printWidth": 80,
		"semi": false,
		"trailingComma": "all",
		"useTabs": true
	}
