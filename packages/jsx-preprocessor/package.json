{
	"name": "@twind/jsx-preprocessor",
	"author": "MapleLeaf",
	"module": "dist/twind-jsx-preprocessor.esm.js",
	"version": "0.1.0",
	"license": "MIT",
	"main": "dist/index.js",
	"typings": "dist/index.d.ts",
	"scripts": {
		"dev": "tsdx watch",
		"build": "tsdx build",
		"test": "tsdx test",
		"lint": "tsdx lint",
		"prepare": "tsdx build",
		"size": "size-limit",
		"analyze": "size-limit --why"
	},
	"dependencies": {
		"@babel/core": "^7.13.8",
		"@babel/helper-annotate-as-pure": "^7.12.13",
		"@babel/traverse": "^7.13.0"
	},
	"devDependencies": {
		"@babel/plugin-syntax-jsx": "^7.12.13",
		"@babel/plugin-syntax-typescript": "^7.12.13",
		"husky": "^5.1.3",
		"tsdx": "^0.14.1",
		"tslib": "^2.1.0",
		"typescript": "^4.2.2"
	},
	"files": [
		"dist",
		"src"
	],
	"engines": {
		"node": ">=14"
	},
	"husky": {
		"hooks": {
			"pre-commit": "tsdx lint"
		}
	},
	"prettier": {
		"printWidth": 80,
		"semi": false,
		"trailingComma": "all",
		"useTabs": true
	}
}
